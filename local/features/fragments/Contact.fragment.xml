<core:FragmentDefinition 
    xmlns="sap.m" 
    xmlns:l="sap.ui.layout" 
    xmlns:core="sap.ui.core" 
    xmlns:mvc="sap.ui.core.mvc">
    <Link text="{i18n>Global.Link.ParkingOffice}" class="sapUiSmallMarginBottom link" wrapping="true" />
    <Text text="{i18n>Global.Text.Located}" class="sapUiTinyMarginBottom bold-text" />
    <Text text="{i18n>Global.Text.Building}" class="sapUiTinyMarginBottom" />
    <Text text="{i18n>Global.Text.Hours}" class="sapUiTinyMarginBottom" />
    <HBox>
        <Text text="{i18n>Global.Text.Email}" class="sapUiTinyMarginBottom " />
        <Link text="{i18n>Global.Text.EmailAddress}" class="sapUiTinyMarginBegin " wrapping="true" />
    </HBox>
    <Text text="{i18n>Global.Text.Phone}" class="sapUiTinyMarginBottom" />
</core:FragmentDefinition>